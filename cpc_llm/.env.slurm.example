# SLURM job environment setup
# Copy to ~/.env.slurm and fill in values for your cluster.
# This file is sourced by SLURM jobs before running pipeline commands.

# WandB configuration
export WANDB_API_KEY=""
export WANDB_BASE_URL=""                 # e.g. https://genentech.wandb.io
export WANDB_INSECURE_DISABLE_SSL=true   # set if using self-signed certs

# AWS credentials (for S3 output storage)
export AWS_ACCESS_KEY_ID="$AWS_ACCESS_KEY_ID"
export AWS_SECRET_ACCESS_KEY="$AWS_SECRET_ACCESS_KEY"
